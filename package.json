{
  "name": "ember-spread",
  "version": "7.0.0",
  "description": "Dynamic options for dynamic components",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- npm install && COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-spread.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Steven Glanzer (https://github.com/sglanzer)",
  "contributors": [
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.7.0",
    "chai-jquery": "^2.0.0",
    "ember-browserify": "^1.2.0",
    "ember-cli": "~3.3.0",
    "ember-cli-chai": "^0.4.3",
    "ember-cli-code-coverage": "1.0.0-beta.5",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-htmlbars": "^2.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "^0.15.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-code-snippet": "1.7.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-hook": "1.4.2",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^0.7.0",
    "ember-source": "~3.3.1",
    "ember-test-utils": "^8.1.1",
    "ember-try": "1.0.0",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-prop-types": "^8.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
