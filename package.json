{
  "name": "ember-spread",
  "version": "1.1.3",
  "description": "Dynamic options for dynamic components",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": "git@github.com:ciena-blueplanet/ember-spread.git",
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "Steven Glanzer (https://github.com/sglanzer)",
  "contributors": [
    "Matthew Dahl (https://github.com/sandersky)"
  ],
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.5.0",
    "chai-jquery": "^2.0.0",
    "ember-cli": "~2.11.0",
    "ember-cli-app-version": "^2.0.1",
    "ember-cli-chai": "0.3.2",
    "ember-cli-code-coverage": "0.3.5",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "0.3.6",
    "ember-cli-inject-live-reload": "^1.6.1",
    "ember-cli-mocha": "0.13.2",
    "ember-cli-test-loader": "^1.1.1",
    "ember-cli-uglify": "^1.2.0",
    "ember-code-snippet": "^1.8.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.1.1",
    "ember-hook": "^1.4.1",
    "ember-load-initializers": "0.6.3",
    "ember-prop-types": "^3.10.2",
    "ember-resolver": "^2.1.1",
    "ember-sinon": "0.6.0",
    "ember-test-utils": "^1.10.3",
    "loader.js": "^4.1.0",
    "sinon-chai": "^2.8.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.6",
    "ember-hash-helper-polyfill": "0.1.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
